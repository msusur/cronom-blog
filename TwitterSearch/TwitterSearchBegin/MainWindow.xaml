<Window x:Class="TwitterSearchBegin.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="260" Width="400">
    
    <DockPanel Height="Auto" Width="Auto" Loaded="DockPanel_Loaded_1" >
        <TextBox x:Name="txtKeyword" Height="30" Text="" Width="Auto" DockPanel.Dock="Top"/>
        <ListBox ItemsSource="{Binding}" x:Name="listResult" Height="200" Width="Auto" DockPanel.Dock="Bottom"/>        
    </DockPanel>
    
</Window>
